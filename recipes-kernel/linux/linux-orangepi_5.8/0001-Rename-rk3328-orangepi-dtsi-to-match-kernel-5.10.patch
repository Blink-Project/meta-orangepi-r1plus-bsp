From f1b7788256c13f34a9ae53fa8196a614cdaf9481 Mon Sep 17 00:00:00 2001
From: Alexi Demers <alexidemers@gmail.com>
Date: Tue, 17 Jan 2023 21:11:21 -0500
Subject: [PATCH] Rename rk3328-orangepi dtsi to match kernel 5.10, and adjust
 node names

---
 ...r1plus-common.dtsi => rk3328-orangepi-r1-plus-common.dtsi} | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)
 rename arch/arm64/boot/dts/rockchip/{rk3328-orangepi-r1plus-common.dtsi => rk3328-orangepi-r1-plus-common.dtsi} (99%)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1plus-common.dtsi b/arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1-plus-common.dtsi
similarity index 99%
rename from arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1plus-common.dtsi
rename to arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1-plus-common.dtsi
index 791ce400c4..cadd0cd5b0 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1plus-common.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1-plus-common.dtsi
@@ -251,7 +251,7 @@
 	clock_in_out = "input";
 	pinctrl-names = "default";
 	pinctrl-0 = <&rgmiim1_pins>;
-	phy-handle = <&rtl8211e>;
+	phy-handle = <&rtl8211f>;
 	phy-mode = "rgmii";
 	phy-supply = <&vcc_phy>;
 	snps,reset-active-low;
@@ -269,7 +269,7 @@
 		#address-cells = <1>;
 		#size-cells = <0>;
 
-		rtl8211e: phy@0 {
+		rtl8211f: phy@0 {
 			reg = <0>;
 			reset-assert-us = <10000>;
 			reset-deassert-us = <30000>;
-- 
2.34.1

